      <h1>My Products</h1>
      <p>List off all of our Products</p>
      <section>
        <% for (let product of prods) { %>
        <h2><%= product.title %></h2>
        <img
          src="https://cdn.pixabay.com/photo/2023/12/08/23/46/cat-8438334_1280.jpg"
          alt="A Book from ejs file"
        />
        <% } %>
      </section>
    </main>
    <% } else { %>
    <p>No products found.</p>
    <% } %>
